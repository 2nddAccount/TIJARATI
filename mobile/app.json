{
  "expo": {
    "name": "TIJARATI",
    "slug": "tijarati",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F766E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oussama.tijarati"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F766E"
      },
      "package": "com.tijarati",
      "softwareKeyboardLayoutMode": "pan"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Tijarati to save backup files.",
          "savePhotosPermission": "Allow Tijarati to save backup files to your device.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}